<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Anggota5">

    <ImageView
        android:id="@+id/foto_yusuf"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/foto_yusuf"
        app:srcCompat="@drawable/fotoyusuf" />

    <View
        android:layout_width="300dp"
        android:layout_height="5dp"
        android:layout_gravity="center"
        android:layout_marginTop="9dp"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/nama_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center"
        android:text="@string/nama_5"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/nim_5"
        android:layout_width="279dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="40dp"
        android:autofillHints="@string/nim_5"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/nim_5"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text="@string/nim_5"
        android:textSize="24sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/email_5"
        android:layout_width="279dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="7dp"
        android:autofillHints="@string/email_5"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/email_5"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        android:text="@string/email_5"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/alamat_5"
        android:layout_width="279dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:autofillHints="@string/alamat_5"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/alamat_5"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text="@string/alamat_5"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/list5"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="125dp"
        android:layout_marginTop="40dp"
        android:text="@string/button_list5" />
</LinearLayout>